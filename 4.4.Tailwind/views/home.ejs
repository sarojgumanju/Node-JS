<%- include('partials/head') %>
</head>
  <body>
    <%- include('partials/nav') %>
    <main
      class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl"
    >
      <h2 class="text-5xl text-gray-800 font-bold text-center mb-6">
        Here are our registered homes
      </h2>
      <ul class="list-disc list-inside space-y-2">
        <% registeredHome.forEach((home) => { %>
        <li
          class="text-gray-700 hover:text-red-500 transition-colors duration-300"
        >
          <%= home.houseName %>
        </li>
        <% }) %>
      </ul>
    </main>
  </body>
</html>
